<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Gens Nul</title>
</head>
<style>
    article {
        background-color: red;
    }
</style>

<body>

<article>
    {% if show_zebi. published %}
    <h1>Gens Nul</h1>
        <ul>
            {% if show_zebi .published %}
                <li>{{ show_zebi. title }}</li>
                <img src="{{ show_zebi. image }}" alt="Quelqu'un de nul">
                {{ show_zebi. publishedDate|date("d/m/Y") }}
            {% endif %}

        </ul>

    {% endif %}



</article>

</body>
</html>